		<section id="standard_lr_show">
			<article>
				<section>
					<p class="content_title">
						<span><img src="http://www.digxy.com/Images/image/PDF.png" /><strong>${data.res.resname}</strong></span>
					</p>
					<p class="title_seconder">分类：     浏览：${data.res.clicktimes}次     </p>
					<div class="content_show"  id="flashcontent" >
					
					</div>
					  <script type="text/javascript">
                                jq('#flashcontent').FlexPaperViewer(
            { config : {

                SWFFile : 'upload/${data.res.respath}',

                Scale : 0.6,
                ZoomTransition : 'easeOut',
                ZoomTime : 0.5,
                ZoomInterval : 0.2,
                FitPageOnLoad : true,
                FitWidthOnLoad : false,
                FullScreenAsMaxWindow : false,
                ProgressiveLoading : true,
                MinZoomSize : 0.2,
                MaxZoomSize : 5,
                SearchMatchAll : false,
                InitViewMode : 'Portrait',
                RenderingOrder : 'flash',
                StartAtPage : '',
				jsDirectory : 'tools/flex/',
                ViewModeToolsVisible : true,
                ZoomToolsVisible : true,
                NavToolsVisible : true,
                CursorToolsVisible : true,
                SearchToolsVisible : true,
                WMode : 'window',
                localeChain: 'en_US'
            }}
    );
    
    </script>
					<p></p>
