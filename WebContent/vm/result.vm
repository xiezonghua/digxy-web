							#end
							#else
								<li>暂无数据</li>
							#end	
						</ul>
					</nav>
				</section>
				<section class="resource_list">
					<p class="clearfix"><span class="layout_left font_bold">分享资料</span><span class="layout_right"><a href="#">更多</a></span></p>
					<nav>
						<ul>
							#if($data.uploaderHots.size() > 0)
							#foreach($elem in ${data.uploaderHots})
					
							<li><a href="page/result?id=${elem.id}">${elem.resname}</a></li>
							#end
							#else
								<li>暂无数据</li>
							#end	
